{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页",
				// 新增首页配置
				// 导航栏样式 default/custom custom取消原生导航栏
				"navigationStyle": "custom",
				// h5端和app端执行
				"app-plus": {
					"scrollIndicator": "none",
					"titleNView": "none"
				}
			}
		},
		{
			"path": "pages/interview-question/interview-question",
			"style": {
				"navigationBarTitleText": "面试题",
				// 新增配置
				"navigationStyle": "custom",
				"backgroundColor": "#f0f3f7",
				// 上拉刷新和下拉加载
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/interview-process/interview-process",
			"style": {
				"navigationBarTitleText": "面试流程",
				// 新增配置
				"navigationStyle": "custom",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/my-contract/my-contract",
			"style": {
				"navigationBarTitleText": "协议",
				"navigationStyle": "custom",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/my-card/my-card",
			"style": {
				// "navigationBarTitleText": "我的",
				"navigationStyle": "custom",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/show-contract/show-contract",
			"style": {
				"navigationBarTitleText": "",
				"navigationStyle": "custom",
				"enablePullDownRefresh": false
			}
		}
		
	],
	// 全局样式
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {},
	// 新建TabBar字段
	"tabBar":{
		"custom":true,
		"list": [{
			"pagePath": "pages/index/index",
			"text": "首页"
		},{
			"pagePath": "pages/interview-question/interview-question",
			"text": "面试题"
		},{
			"pagePath": "pages/interview-process/interview-process",
			"text": "面试流程"
		},{
			"pagePath": "pages/my-contract/my-contract",
			"text": "协议"
		},{
			"pagePath": "pages/my-card/my-card",
			"text": "我的"
		}]
	}
}
